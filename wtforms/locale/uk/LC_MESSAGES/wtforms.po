#: wtforms/validators.py:55
#, python-format
msgid "Invalid field name '%s'."
msgstr "Невірне ім'я поля '%s'."

#: wtforms/validators.py:63
#, python-format
msgid "Field must be equal to %(other_name)s."
msgstr "Поле має співпадати з %(other_name)s."

#: wtforms/validators.py:96
#, python-format
msgid "Field must be at least %(num)d character long."
msgid_plural "Field must be at least %(num)d characters long."
msgstr[0] "Значення поля має містити не менше %(num)d символа."
msgstr[1] "Значення поля має містити не менше ніж %(num)d символи."
msgstr[2] "Значення поля має містити не менше ніж %(num)d символів."

#: wtforms/validators.py:99
#, python-format
msgid "Field cannot be longer than %(num)d character."
msgid_plural "Field cannot be longer than %(num)d characters."
msgstr[0] "Значення поля має містити не більше %(num)d символа."
msgstr[1] "Значення поля має містити не більше ніж %(num)d символи."
msgstr[2] "Значення поля має містити не більше ніж %(num)d символів."

#: wtforms/validators.py:102
#, python-format
msgid "Field must be between %(min)d and %(max)d characters long."
msgstr "Значення поля має містити від %(min)d до %(max)d символів."

#: wtforms/validators.py:138
#, python-format
msgid "Number must be at least %(min)s."
msgstr "Число має бути щонайменше %(min)s."

#: wtforms/validators.py:140
#, python-format
msgid "Number must be at most %(max)s."
msgstr "Число має бути щонайбільше %(max)s."

#: wtforms/validators.py:142
#, python-format
msgid "Number must be between %(min)s and %(max)s."
msgstr "Число має бути між %(min)s та %(max)s."

#: wtforms/validators.py:198 wtforms/validators.py:233
msgid "This field is required."
msgstr "Це поле є обов'язковим."

#: wtforms/validators.py:264
msgid "Invalid input."
msgstr "Введено невірно."

#: wtforms/validators.py:286
msgid "Invalid email address."
msgstr "Невірна електронна адреса."

#: wtforms/validators.py:318
msgid "Invalid IP address."
msgstr "Невірна IP адреса."

#: wtforms/validators.py:367
msgid "Invalid Mac address."
msgstr "Невірна Mac адреса."

#: wtforms/validators.py:393
msgid "Invalid URL."
msgstr "Невірний URL."

#: wtforms/validators.py:412
msgid "Invalid UUID."
msgstr "Невірний UUID."

#: wtforms/validators.py:440
#, python-format
msgid "Invalid value, must be one of: %(values)s."
msgstr "Значення невірне, має бути одним з: %(values)s."

#: wtforms/validators.py:472
#, python-format
msgid "Invalid value, can't be any of: %(values)s."
msgstr "Значення невірне, не може бути одним з: %(values)s."

#: wtforms/csrf/core.py:83 wtforms/ext/csrf/form.py:47
msgid "Invalid CSRF Token"
msgstr "Невірний CSRF токен."

#: wtforms/csrf/session.py:61 wtforms/ext/csrf/session.py:58
msgid "CSRF token missing"
msgstr "CSRF токен відсутній"

#: wtforms/csrf/session.py:69 wtforms/ext/csrf/session.py:66
msgid "CSRF failed"
msgstr "Помилка CSRF"

#: wtforms/csrf/session.py:74 wtforms/ext/csrf/session.py:71
msgid "CSRF token expired"
msgstr "CSRF токен прострочений"

