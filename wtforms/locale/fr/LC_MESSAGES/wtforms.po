#: wtforms/validators.py:55
#, python-format
msgid "Invalid field name '%s'."
msgstr "Nom de champ invalide « %s »."

#: wtforms/validators.py:63
#, python-format
msgid "Field must be equal to %(other_name)s."
msgstr "Le champ doit être égal à %(other_name)s."

#: wtforms/validators.py:96
#, python-format
msgid "Field must be at least %(min)d character long."
msgid_plural "Field must be at least %(min)d characters long."
msgstr[0] "Le champ doit faire au moins %(min)d caractère."
msgstr[1] "Le champ doit faire au moins %(min)d caractères."

#: wtforms/validators.py:99
#, python-format
msgid "Field cannot be longer than %(max)d character."
msgid_plural "Field cannot be longer than %(max)d characters."
msgstr[0] "Le champ ne peut pas faire plus de %(max)d caractère."
msgstr[1] "Le champ ne peut pas faire plus de %(max)d caractères."

#: wtforms/validators.py:102
#, python-format
msgid "Field must be between %(min)d and %(max)d characters long."
msgstr ""
"La longueur du champ doit être comprise entre %(min)d et %(max)d caractères."

#: wtforms/validators.py:138
#, python-format
msgid "Number must be at least %(min)s."
msgstr "Le nombre doit être d'au moins %(min)s."

#: wtforms/validators.py:140
#, python-format
msgid "Number must be at most %(max)s."
msgstr "Le nombre doit être au plus %(max)s."

#: wtforms/validators.py:142
#, python-format
msgid "Number must be between %(min)s and %(max)s."
msgstr "Le nombre doit être entre %(min)s et %(max)s."

#: wtforms/validators.py:198 wtforms/validators.py:233
msgid "This field is required."
msgstr "Ce champ est requis."

#: wtforms/validators.py:264
msgid "Invalid input."
msgstr "Entrée invalide."

#: wtforms/validators.py:286
msgid "Invalid email address."
msgstr "Adresse email invalide."

#: wtforms/validators.py:318
msgid "Invalid IP address."
msgstr "Adresse IP invalide."

#: wtforms/validators.py:367
msgid "Invalid Mac address."
msgstr "Adresse Mac invalide."

#: wtforms/validators.py:393
msgid "Invalid URL."
msgstr "URL invalide."

#: wtforms/validators.py:412
msgid "Invalid UUID."
msgstr "UUID invalide."

#: wtforms/validators.py:440
#, python-format
msgid "Invalid value, must be one of: %(values)s."
msgstr "Valeur invalide, doit être parmi : %(values)s."

#: wtforms/validators.py:472
#, python-format
msgid "Invalid value, can't be any of: %(values)s."
msgstr "Valeur invalide, ne peut être parmi : %(values)s."

#: wtforms/csrf/core.py:83 wtforms/ext/csrf/form.py:47
msgid "Invalid CSRF Token"
msgstr "CSRF Token invalide"

#: wtforms/csrf/session.py:61 wtforms/ext/csrf/session.py:58
msgid "CSRF token missing"
msgstr "CSRF token manquant"

#: wtforms/csrf/session.py:69 wtforms/ext/csrf/session.py:66
msgid "CSRF failed"
msgstr "CSRF échoué"

#: wtforms/csrf/session.py:74 wtforms/ext/csrf/session.py:71
msgid "CSRF token expired"
msgstr "CSRF token expiré"

